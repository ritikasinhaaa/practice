#include<iostream>
using namespace std;
int swap(int b[],int c[])
{
    int temp;
    
}

int main()
{
    int i,n,a[50],count=0;
    cout<<"enter n";
    cin>>n;
    cout<<"enter elements";
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        if(a[i]!=0)
        {
            swap(a[count++],a[i]);
        }
    }
    for(i=0;i<n;i++)
    {
        cout<<a[i];
    }
}

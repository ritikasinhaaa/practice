#include<iostream>
using namespace std;
int main()
{
    int i,a[50],n,min,max;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    min=a[0];
    max=a[0];
    for(i=0;i<n;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
        else if(a[i]<min)
        {
            min=a[i];
        }
    }
    cout<<max<<"is max"<<endl;
    cout<<min<<"is min";
    
}

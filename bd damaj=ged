#include<iostream>
using namespace std;
int binarys(int ar,int n,int a[],int x)
{
    int mid;
    mid=n/2;
    if(a[mid]==x)
    {
       return mid+1; 
    }
    else if(a[mid]>x)
    {
       return binarys(0,mid-1,a,x); 
    }
    else{
        return binarys(mid+1,n,a,x);
    }
}
int main()
{
    int i, n,a[50],ele,c;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>ele;
    c=binarys(0,n-1,a,ele);
    cout<<"ele found at "<<c<<" position";
    return 0;
}
